{
  "name": "@wikia/react-design-system",
  "version": "0.4.23",
  "description": "Reusable React components based on Wikia's Design System",
  "main": "dist/index.js",
  "repository": "git@github.com:Wikia/react-design-system.git",
  "license": "PRIVATE",
  "private": true,
  "dependencies": {
    "design-system": "git://github.com/Wikia/design-system.git#12.0.0",
    "lodash.uniqueid": "^4.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "scripts": {
    "build": "yarn build:styleguidist && yarn build:lib",
    "build:lib": "rimraf dist && cross-env NODE_ENV=production webpack",
    "build:styleguidist": "cross-env NODE_ENV=production styleguidist build",
    "coverage": "jest --coverage",
    "coveralls": "yarn coverage --runInBand && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dev": "concurrently --names \"SERVER,TESTS\" --prefix \"{time}|{name}\" \"yarn styleguidist:server\" \"yarn test:watch\"",
    "lint:css": "stylelint \"components/*.css\"",
    "lint:fix": "stylelint \"components/*.css\" --fix",
    "lint:js": "eslint .",
    "lint": "yarn lint:js && yarn lint:css",
    "release": "yarn release:patch",
    "release:major": "git checkout master && git pull && yarn build && git add . && yarn version --new-version major && git push --follow-tags",
    "release:minor": "git checkout master && git pull && yarn build && git add . && yarn version --new-version minor && git push --follow-tags",
    "release:patch": "git checkout master && git pull && yarn build && git add . && yarn version --new-version patch && git push --follow-tags",
    "styleguidist:server": "styleguidist server",
    "test": "yarn lint && yarn test:run && yarn coverage",
    "test:run": "jest",
    "test:watch": "jest --watchAll --notify"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.3.0",
    "eslint-config-fandom": "git://github.com/Wikia/eslint-config-fandom.git#2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "react-styleguidist": "^7.2.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.3",
    "sinon": "^6.1.5",
    "stylelint": "^9.1.3",
    "stylelint-config-fandom": "git://github.com/Wikia/stylelint-config-fandom.git#0.0.1",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
